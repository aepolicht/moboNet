<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity$DummySectionFragment"
        >

    <ImageView
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            android:id="@+id/imageView3"
            android:background="#dddddd"
            android:layout_gravity="center|bottom"
            android:layout_below="@+id/imageView"
            android:layout_centerHorizontal="true"/>

    <ImageView
            android:layout_width="fill_parent"
            android:layout_height="135dp"
            android:id="@+id/imageView"
            android:layout_alignParentLeft="true"
            android:background="#C1C1C1"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            />

    <ImageView
            android:layout_width="fill_parent"
            android:layout_height="220dp"
            android:id="@+id/imageView2"
            android:background="#dddddd"
            android:layout_gravity="center|top"
            android:layout_marginTop="0dp"
            android:layout_below="@+id/imageView"
            android:layout_centerHorizontal="true"/>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="120dp"
            android:layout_height="185dp"
            android:layout_gravity="left|top"
            android:layout_marginTop="18dp"
            android:layout_marginLeft="20dp"
            android:background="#c1c1c1"
            >

        <RadioGroup
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="0dp"
                android:layout_marginTop="0dp"
                android:id="@+id/radioGroup"
                android:layout_marginLeft="10dp"
                style="@android:style/TextAppearance.DeviceDefault.Small">

            <RadioButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Solid"
                    android:id="@+id/radioButton"
                    android:checked="true"
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:onClick="radioChange"/>

            <RadioButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Strobe"
                    android:id="@+id/radioButton2"
                    android:checked="false"
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:onClick="radioChange"/>

            <RadioButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Pulse"
                    android:id="@+id/radioButton3"
                    android:checked="false"
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:onClick="radioChange"/>

            <RadioButton
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Fade"
                    android:id="@+id/radioButton4"
                    android:checked="false"
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:onClick="radioChange"/>

            <RadioButton
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Ambiance"
                    android:id="@+id/radioButton5"
                    android:checked="false"
                    android:onClick="radioChange"/>

            <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Music"
                    android:id="@+id/radioButton6"
                    android:checked="false"
                    style="@android:style/TextAppearance.DeviceDefault.Small"
                    android:onClick="radioChange"/>
        </RadioGroup>

    </LinearLayout>

    <RelativeLayout
            android:layout_width="185dp"
            android:layout_height="185dp"
            android:layout_gravity="right|top"
            android:layout_marginRight="17dp"
            android:layout_marginTop="18dp"
            android:background="#c1c1c1"
            >

        <SeekBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/seekBar"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:max="255"
                android:clickable="true"
                />

        <SeekBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/seekBar2"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="15dp"
                android:max="255"
                />

        <SeekBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/seekBar3"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:max="255"
                />

    </RelativeLayout>

    <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_gravity="center"
            android:layout_marginTop="35dp">

        <ToggleButton
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton"
                android:checked="false"
                android:onClick="clickA"/>

        <ToggleButton
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton2"
                android:checked="false"
                android:onClick="clickB"/>

        <ToggleButton
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton3"
                android:onClick="clickC"/>

        <ToggleButton
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton4"
                android:checked="false"
                android:clickable="true"
                android:focusableInTouchMode="false"
                android:focusable="false"
                android:onClick="clickD"/>
    </LinearLayout>

    <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="109dp"
            android:layout_gravity="center_horizontal|bottom"
            android:layout_marginBottom="20dp">

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton"
                android:background="#ffffff"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton2"
                android:background="#010101"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton"
                android:layout_marginLeft="11dp"
                android:focusable="false"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton3"
                android:background="#a90000"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton2"
                android:layout_marginLeft="11dp"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton4"
                android:background="#de7900"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton3"
                android:layout_marginLeft="11dp"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton5"
                android:background="#ffea00"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/imageButton4"
                android:layout_marginLeft="11dp"
                android:clickable="true"
                android:focusable="true"
                android:focusableInTouchMode="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton6"
                android:background="#008700"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton7"
                android:background="#0000c6"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/imageButton3"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton8"
                android:background="#0091b4"
                android:layout_alignParentBottom="true"
                android:layout_toLeftOf="@+id/imageButton3"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton9"
                android:background="#7900be"
                android:layout_alignTop="@+id/imageButton8"
                android:layout_alignLeft="@+id/imageButton4"
                android:clickable="true"/>

        <ImageButton
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/imageButton10"
                android:background="#aa002c"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/imageButton5"
                android:clickable="true"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignBottom="@+id/imageButton"
                android:layout_toLeftOf="@+id/imageButton2"
                android:onClick="colorClick1"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button2"
                android:layout_alignLeft="@+id/imageButton2"
                android:layout_alignBottom="@+id/imageButton2"
                android:layout_toLeftOf="@+id/imageButton3"
                android:onClick="colorClick2"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button3"
                android:layout_alignLeft="@+id/imageButton3"
                android:layout_alignBottom="@+id/imageButton3"
                android:layout_toLeftOf="@+id/imageButton4"
                android:onClick="colorClick3"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button4"
                android:layout_alignLeft="@+id/imageButton4"
                android:layout_alignBottom="@+id/imageButton4"
                android:layout_toLeftOf="@+id/imageButton5"
                android:onClick="colorClick4"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button5"
                android:layout_alignLeft="@+id/imageButton5"
                android:layout_alignBottom="@+id/imageButton5"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/imageButton5"
                android:onClick="colorClick5"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button6"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_toLeftOf="@+id/imageButton2"
                android:onClick="colorClick6"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button7"
                android:layout_alignLeft="@+id/imageButton8"
                android:layout_alignBottom="@+id/button6"
                android:layout_toLeftOf="@+id/imageButton3"
                android:onClick="colorClick7"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button8"
                android:layout_alignLeft="@+id/imageButton7"
                android:layout_alignBottom="@+id/button7"
                android:layout_toLeftOf="@+id/imageButton4"
                android:onClick="colorClick8"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button9"
                android:layout_alignLeft="@+id/imageButton9"
                android:layout_alignBottom="@+id/button8"
                android:layout_toLeftOf="@+id/imageButton5"
                android:onClick="colorClick9"/>

        <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button10"
                android:layout_alignLeft="@+id/imageButton10"
                android:layout_alignBottom="@+id/button9"
                android:layout_alignRight="@+id/imageButton10"
                android:onClick="colorClick10"/>
    </RelativeLayout>

</FrameLayout>
